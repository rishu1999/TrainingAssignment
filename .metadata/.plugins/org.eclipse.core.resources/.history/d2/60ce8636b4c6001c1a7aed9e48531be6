package evennumber;
import java.util.*;
public class EvenNumber {


	   public static void main(String []args)
	   {
		   int n=0, i=0;
		   Scanner A = new Scanner(System.in);
		   System.out.print("Enter value of n: ");
		   n = A.nextInt();
		   for(i=1; i<n; i++);
		   {
			   if(i%2==0)
				   System.out.print(i+ " ");
		   }
		   System.out.println();
	   }
	
}


